t?.title}\n            <h5 style={{ fontSize: \"1rem\" }}>By {project?.teamname}</h5>\n            <span className=\"meta\">\n              <em style={{ fontSize: \"0.8rem\" }}>\n                Issued on {new Date(project?.issuedon).toLocaleDateString()}\n              </em>\n            </span>\n          </div>\n        </div>\n      </div>\n\n      <Jumbotron\n        fluid\n        style={{\n          background: \"white\",\n          width: \"100%\",\n          margin: \"auto\",\n          paddingBottom: \"1rem\",\n          paddingLeft: \"2rem\",\n        }}\n      >\n        <Container>\n          <h4>Description:</h4>\n          <p dangerouslySetInnerHTML={{ __html: project?.description }}></p>\n          <h4>Objective:</h4>\n          <p dangerouslySetInnerHTML={{ __html: project?.objective }}></p>\n          <p>\n            <img style={{ width: \"60vw\" }} src={project?.pic} />\n          </p>\n          <h4>Project Status:</h4>\n          <p dangerouslySetInnerHTML={{ __html: project?.status }}></p>\n\n          <h4>Members:</h4>\n          <p>\n            {\" \"}\n            {project?.members.map(function (d, idx) {\n              console.log(d);\n              return <li key={idx}>{d.user.name}</li>;\n            })}\n          </p>\n        </Container>\n        <hr />\n      </Jumbotron>\n    </div>\n  );\n}\n\nexport default SingleProject;\n","/home/ishan/Desktop/AeroClub_Website/client/src/components/Pages/SingleBlog.js",["344","345","346","347"],"import React, { useEffect, useState } from \"react\";\nimport { useHistory, useParams } from \"react-router-dom\";\nimport \"../../css/SingleBlog.css\";\nimport Loading from \"../../Animations/Loading\";\nimport { Container, Jumbotron } from \"react-bootstrap\";\n\nfunction SingleBlog() {\n  const { blogId } = useParams();\n  const [blog, setBlog] = useState(undefined);\n  const history = useHistory();\n\n  const year = {\n    1: \"1st year\",\n    2: \"2nd year\",\n    3: \"3rd year\",\n    4: \"final year\",\n  };\n\n  useEffect(() => {\n    fetch(`/api/blogstoUI/${blogId}`, {\n      method: \"get\",\n    })\n      .then((res) => res.json())\n      .then((data) => {\n        if (data.error) history.push(\"/404\");\n        else setBlog(data);\n      });\n  }, []);\n\n  return (\n    <div>\n      <Loading time={1.5} />\n\n      <div\n        className=\" pagesp\"\n        style={{\n          background: `linear-gradient(rgba(0,0,0,0.4), rgba(0,0,0,.4)), url(${blog?.pic})`,\n        }}\n      >\n        <div className=\"overlayp\">\n          <div className=\"pageTitlep titleBoldp\">\n            {blog?.title}\n            <p className=\"meta\">\n              <em style={{ fontSize: \"0.8rem\" }}>\n                Posted by{\" \"}\n                {blog?.postedBy.linkedin_url !==\n                \"https://www.linkedin.com/in/username/\" ? (\n                  <a href={blog?.postedBy.linkedin_url} target=\"_blank\">\n                    {blog?.postedBy.name}\n                  </a>\n                ) : (\n                  blog?.postedBy.name\n                )}{\" \"}\n                {`(reg. no :- ${\n                  blog?.postedBy.registration_no == \"xxxxxxxx\"\n                    ? \"NA\"\n                    : blog?.postedBy.registration_no\n                }, ${\n                  blog?.postedBy.year == -1\n                    ? \"year - NA\"\n                    : year[blog?.postedBy.year]\n                })`}{\" \"}\n                on {new Date(blog?.publishedAt).toLocaleDateString()}\n              </em>\n            </p>\n          </div>\n        </div>\n      </div>\n      <Jumbotron\n        fluid\n        style={{\n          background: \"white\",\n          width: \"100%\",\n          margin: \"auto\",\n          paddingBottom: \"1rem\",\n          paddingLeft: \"2rem\",\n        }}\n      >\n        <Container>\n          <p dangerouslySetInnerHTML={{ __html: blog?.body }}></p>\n        </Container>\n        <hr />\n      </Jumbotron>\n    </div>\n  );\n}\n\nexport default SingleBlog;\n","/home/ishan/Desktop/AeroClub_Website/client/src/components/Pages/ForgetPassword.js",[],"/home/ishan/Desktop/AeroClub_Website/client/src/components/Pages/Signup.js",["348"],"import React, { useEffect, useRef } from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport \"../../css/Login.css\";\nimport { toast } from \"react-toastify\";\n\nfunction Signup() {\n  document.title = \"Signup | Aero Club\";\n\n  const name = useRef();\n  const email = useRef();\n  const password = useRef();\n  const confirmPassword = useRef();\n  const history = useHistory();\n\n  useEffect(() => {\n    if (localStorage.getItem(\"jwtToken\")) history.push(\"/404\");\n  }, []);\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n\n    if (password.current.value !== confirmPassword.current.value) {\n      toast.warn(\"passwords do not match !\");\n      return;\n    }\n\n    fetch(`/api/signup`, {\n      method: \"post\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify({\n        name: name.current.value,\n        email: email.current.value,\n        password: password.current.value,\n      }),\n    })\n      .then((res) => res.json())\n      .then((data) => {\n        if (data.error) toast.warn(data.error);\n        else {\n          toast.success(data.message);\n          history.push(\"/user/login\");\n          window.location.reload();\n        }\n      });\n  };\n\n  return (\n    <div className=\"login rounded-3\">\n      <div className=\"container\">\n        <div className=\"row\">\n          <div className=\"col-sm-9 col-md-7 col-lg-5 mx-auto\">\n            <div className=\"card card-signin mt-5 mb-3\">\n              <div className=\"card-body\">\n                <h5\n                  className=\"card-title text-center font-weight-bold\"\n                  id=\"heading\"\n                >\n                  Account Signup\n                </h5>\n                <form className=\"form-signin\">\n                  <div className=\"form-label-group\">\n                    <input\n                      type=\"text\"\n                      id=\"inputname\"\n                      className=\"form-control\"\n                      placeholder=\"Name\"\n                      required\n                      autoFocus\n                      ref={name}\n                    />\n                    <label htmlFor=\"inputname\">Name</label>\n                  </div>\n                  <div className=\"form-label-group\">\n                    <input\n                      type=\"email\"\n                      id=\"inputEmail\"\n                      className=\"form-control\"\n                      placeholder=\"Gsuite Email Address\"\n                      required\n                      autoFocus\n                      ref={email}\n                    />\n                    <label htmlFor=\"inputEmail\">Gsuite Email Address</label>\n                  </div>\n                  <div className=\"form-label-group\">\n                    <input\n                      type=\"password\"\n                      id=\"inputPassword\"\n                      className=\"form-control\"\n                      placeholder=\"Password\"\n                      required\n                      ref={password}\n                    />\n                    <label htmlFor=\"inputPassword\">Password</label>\n                    <em style={{ fontSize: \"x-small\" }}>\n                      * password must between 8-15 characters containing at\n                      least one lowercase and one uppercase letter, one numeric\n                      digit, and one special character\n                    </em>\n                  </div>\n                  <div className=\"form-label-group\">\n                    <input\n                      type=\"password\"\n                      id=\"inputretypePassword\"\n                      className=\"form-control\"\n                      placeholder=\"Retype Password\"\n                      required\n                      ref={confirmPassword}\n                    />\n                    <label htmlFor=\"inputretypePassword\">Retype Password</label>\n                  </div>\n                  <div className=\"custom-control custom-checkbox mb-3\">\n                    <input\n                      type=\"checkbox\"\n                      className=\"custom-control-input\"\n                      id=\"customCheck1\"\n                    />\n                    <label\n                      className=\"custom-control-label\"\n                      htmlFor=\"customCheck1\"\n                    >\n                      Remember password\n                    </label>\n                  </div>\n                  <button\n                    className=\"btn btn-lg btn-primary btn-block text-uppercase\"\n                    type=\"submit\"\n                    onClick={handleSubmit}\n                  >\n                    Sign up\n                  </button>\n                  <hr className=\"my-4\" />\n                  <div className=\"para\">\n                    <p>\n                      Already have an account?{\" \"}\n                      <Link to=\"/user/login\">Signin</Link>\n                    </p>\n                    <button type=\"button\" className=\"btn btn-dark\">\n                      <Link\n                        to=\"/\"\n                        style={{ color: \"inherit\", textDecoration: \"none\" }}\n                      >\n                        Back to Home\n                      </Link>\n                    </button>\n                  </div>\n                </form>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Signup;\n","/home/ishan/Desktop/AeroClub_Website/client/src/components/Pages/Login.js",["349"],"import React, { useEffect, useRef } from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport { toast } from \"react-toastify\";\nimport \"../../css/Login.css\";\n\nfunction Login() {\n  document.title = \"Login | Aero Club\";\n\n  const email = useRef();\n  const password = useRef();\n  const history = useHistory();\n\n  useEffect(() => {\n    if (localStorage.getItem(\"jwtToken\")) history.push(\"/404\");\n  }, []);\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n\n    if (localStorage.getItem(\"jwtToken\")) {\n      toast.success(\"User logged In successfully !\");\n      history.push(\"/user/dashboard\");\n    }\n\n    fetch(`/api/signin`, {\n      method: \"post\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify({\n        email: email.current.value,\n        password: password.current.value,\n      }),\n    })\n      .then((res) => res.json())\n      .then((data) => {\n        if (data.error) toast.warn(data.error);\n        else {\n          localStorage.setItem(\"jwtToken\", data.token);\n          toast.success(data.message);\n          history.push(\"/user/dashboard\");\n          window.location.reload();\n        }\n      });\n  };\n\n  return (\n    <div className=\"login\">\n      <div className=\"container\">\n        <div className=\"row\">\n          <div className=\"col-sm-9 col-md-7 col-lg-5 mx-auto\">\n            <div className=\"card card-signin my-5\">\n              <div className=\"card-body\">\n                <h5\n                  className=\"card-title text-center font-weight-bold\"\n                  id=\"heading\"\n                >\n                  Account login\n                </h5>\n                <form className=\"form-signin\">\n                  <div className=\"form-label-group\">\n                    <input\n                      type=\"email\"\n                      id=\"inputEmail\"\n                      className=\"form-control\"\n                      placeholder=\"Gsuite Email Address\"\